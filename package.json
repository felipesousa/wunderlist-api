{
  "name": "wunderlist-api",
  "version": "2.0.0",
  "description": "Wunderlist made easy",
  "main": "index.js",
  "repository": "git@github.com:felipesousa/wunderlist-api.git",
  "author": "Felipe Sousa <felipzbr@gmail.com>",
  "license": "MIT",
  "husky": {
    "hooks": {
      "pre-commit": "yarn run test && lint-staged"
    }
  },
  "scripts": {
    "test": "jest",
    "test:watch": "yarn run test -- --watch",
    "build": "tsc -p tsconfig.json"
  },
  "private": false,
  "devDependencies": {
    "@types/jest": "^25.1.2",
    "@types/node": "^13.7.1",
    "husky": "^4.2.3",
    "jest": "^25.1.0",
    "lint-staged": "^10.0.7",
    "prettier": "^1.19.1",
    "ts-jest": "^25.2.0",
    "typescript": "^3.7.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      ".ts": "ts-jest"
    },
    "testRegex": ".*\\.spec\\.ts$"
  },
  "lint-staged": {
    "src/**/*.ts": [
      "prettier --write --no-semi --single-quote --trailing-comma es5"
    ]
  }
}
